{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "react/jsx-filename-extension": 0,
    "semi": ["error", "never"],
    "linebreak-style": 0,
    "indent": ["error", 4],
    "react/jsx-indent": ["error", 4]
  }
}
